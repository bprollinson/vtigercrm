<RDF:RDF xmlns:RDF="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:chrome="http://www.mozilla.org/rdf/chrome#">
  <!--list packages being supplied by this jar-->
  <RDF:Seq about="urn:mozilla:package:root">
    <RDF:li resource="urn:mozilla:package:vtiger"/>
  </RDF:Seq>
    <!--package information-->	
    <RDF:Description about="urn:mozilla:package:vtiger"
        chrome:displayName="VTiger CRM"
	chrome:author="vtiger team"
        chrome:authorURL="http://www.vtiger.com/"
	chrome:name="vtiger"
        chrome:extension="true"
	chrome:description="Tool bar for vtigerCRM"
        chrome:settingsURL="">
  </RDF:Description>
    <!--navigator overlay-->	
    <RDF:Seq about="urn:mozilla:overlays">
		<RDF:li resource="chrome://navigator/content/navigator.xul"/>
		<RDF:li resource="chrome://browser/content/browser.xul"/>
	</RDF:Seq>

	<RDF:Seq about="chrome://navigator/content/navigator.xul">
		<RDF:li>chrome://vtiger/content/vtigertoolbar.xul</RDF:li>
	</RDF:Seq>

 	<RDF:Seq about="chrome://browser/content/browser.xul">
		<RDF:li>chrome://vtiger/content/vtigertoolbar.xul</RDF:li>
	  </RDF:Seq>

</RDF:RDF>




